{
  "name": "infogare",
  "version": "1.3.5",
  "private": true,
  "scripts": {
    "postinstall": "node ./tools/prepare.js && node ./tools/busLines.js && node ./tools/transportsLines.js && node ./tools/airports.js",
    "start-desktop": "node node_modules/electron/cli.js -r babel-register ./electron.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "run-android": "react-native run-android",
    "relaunch-apk-privileged": "yarn build-apk-privileged && yarn install-apk && yarn start-apk && yarn see-logs-apk",
    "relaunch-apk": "yarn build-apk && yarn install-apk && yarn start-apk && yarn see-logs-apk",
    "build-apk": "cd android && ./gradlew assembleRelease",
    "build-apk-privileged": "cd android && ./gradlew assemblePRIVILEGEDRelease",
    "install-apk": "adb uninstall org.toilelibre.libe.trains && adb install android/app/build/outputs/apk/app-release.apk",
    "start-apk": "adb shell am start -n org.toilelibre.libe.trains/.SplashScreenActivity",
    "see-logs-apk": "adb logcat | grep ReactNative",
    "singleCommit": "git add . && git commit -m \"detached\" && git reset --soft origin/expo && git commit -m \"detached\" && git push -f",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "storybook-native": "storybook start -p 7007",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "latest",
    "dom-parser": "latest",
    "geojson-utils": "latest",
    "global": "latest",
    "html-entities": "latest",
    "moment": "latest",
    "proj4": "latest",
    "prop-types": "latest",
    "react": "latest",
    "react-native": "latest",
    "react-native-crypto": "latest",
    "react-native-fs": "latest",
    "react-native-randombytes": "latest",
    "remove-accents": "latest"
  },
  "devDependencies": {
    "@storybook/addon-actions": "latest",
    "@storybook/addon-links": "latest",
    "@storybook/react": "latest",
    "@storybook/react-native": "latest",
    "babel-jest": "latest",
    "babel-polyfill": "latest",
    "babel-preset-react-native": "latest",
    "babel-register": "latest",
    "chalk": "latest",
    "cross-env": "latest",
    "download": "latest",
    "electron": "latest",
    "electron-react-devtools": "latest",
    "event-stream": "latest",
    "jest": "latest",
    "nock": "latest",
    "react-dom": "latest",
    "react-native-cli": "latest",
    "react-native-web": "latest",
    "react-test-renderer": "latest",
    "unzip2": "latest",
    "react-art": "latest"
  },
  "jest": {
    "preset": "react-native"
  }
}
